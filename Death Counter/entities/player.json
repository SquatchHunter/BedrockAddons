{
	"format_version":"1.16.0",
	"minecraft:entity":{
		"description":{
			"identifier":"minecraft:player",
			"is_spawnable":false,
			"animations":{
				"death_count":"controller.animation.player.death_count"
			},
			"is_summonable":false,
			"scripts":{
				"animate":[
					"death_count"
				]
			},
			"is_experimental":false
		},
		"component_groups":{
			"minecraft:raid_trigger":{
				"minecraft:raid_trigger":{
					"triggered_event":{
						"event":"minecraft:remove_raid_trigger",
						"target":"self"
					}
				},
				"minecraft:spell_effects":{
					"remove_effects":"bad_omen"
				}
			},
			"minecraft:add_bad_omen":{
				"minecraft:spell_effects":{
					"add_effects":[
						{
							"duration":6000,
							"display_on_screen_animation":true,
							"effect":"bad_omen"
						}
					]
				},
				"minecraft:timer":{
					"looping":false,
					"time_down_event":{
						"event":"minecraft:clear_add_bad_omen",
						"target":"self"
					},
					"time":[0,0]
				}
			},
			"minecraft:clear_bad_omen_spell_effect":{
				"minecraft:spell_effects":{}
			}
		},
		"components":{
			"minecraft:physics":{},
			"minecraft:nameable":{
				"always_show":true,
				"allow_name_tag_renaming":false
			},
			"minecraft:environment_sensor":{
				"triggers":{
					"filters":{
						"all_of":[
							{
								"test":"has_mob_effect",
								"subject":"self",
								"value":"bad_omen"
							},{
								"test":"is_in_village",
								"subject":"self",
								"value":true
							}
						]
					},
					"event":"minecraft:trigger_raid"
				}
			},
			"minecraft:can_climb":{},
			"minecraft:breathable":{
				"inhale_time":3.75,
				"total_supply":15,
				"suffocate_time":-1,
				"generates_bubbles":false
			},
			"minecraft:rideable":
				{
					"family_types":[
						"parrot_tame"
					],
					"seat_count":2,
					"pull_in_entities":true,
					"seats":[
						{
							"position":[0.4,-0.2,-0.1],
							"min_rider_count":0,
							"lock_rider_rotation":0,
							"max_rider_count":0
						},{
							"position":[-0.4,-0.2,-0.1],
							"min_rider_count":1,
							"lock_rider_rotation":0,
							"max_rider_count":2
						}
					]
				},
				"minecraft:type_family":{
					"family":["player"]
				},
				"minecraft:collision_box":{
					"width":0.6,
					"height":1.8
				},
				"minecraft:movement":{
					"value":0.1
				},
				"minecraft:experience_reward":{
					"on_death":"Math.Min(query.player_level * 7, 100)"
				},
				"minecraft:loot":{
					"table":"loot_tables/empty.json"
				},
				"minecraft:insomnia":{
					"days_until_insomnia":3
				},
				"minecraft:hurt_on_condition":{
				"damage_conditions":[
					{
						"damage_per_tick":4,
						"cause":"lava",
						"filters":{
							"test":"in_lava",
							"subject":"self",
							"value":true,
							"operator":"=="
						}
					}
				]
			},
			"minecraft:scaffolding_climber":{},
			"minecraft:player.level":{
				"max":24791,
				"value":0
			},
			"minecraft:player.experience":{
				"max":1,
				"value":0
			},
			"minecraft:pushable":{
				"is_pushable":false,
				"is_pushable_by_piston":true
			},
			"minecraft:is_hidden_when_invisible":{},
			"minecraft:attack":{
				"damage":1
			},
			"minecraft:player.exhaustion":{
				"max":4,
				"value":0
			},
			"minecraft:player.saturation":{
				"value":20
			}
		},
		"events":{
			"minecraft:remove_raid_trigger":{
				"remove":{
					"component_groups":[
						"minecraft:raid_trigger"
					]
				}
			},
			"minecraft:trigger_raid":{
				"add":{
					"component_groups":[
						"minecraft:raid_trigger"
					]
				}
			},
			"minecraft:gain_bad_omen":{
				"add":{
					"component_groups":[
						"minecraft:add_bad_omen"
					]
				}
			},
			"minecraft:clear_add_bad_omen":{
				"add":{
					"component_groups":[
						"minecraft:clear_bad_omen_spell_effect"
					]
				},
				"remove":{
					"component_groups":[
						"minecraft:add_bad_omen"
					]
				}
			}
		}
	}
}